{
  "name": "angular2-draggable-demo",
  "version": "1.2.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4203",
    "build": "ng build --prod --base-href ./",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build:lib": "ng build angular2-draggable",
    "build:css": "node scripts/build-scss.js",
    "build:prod": "ng build angular2-draggable --prod",
    "update:md": "cpr README.md dist/angular2-draggable/README.md -o",
    "dist": "run-s build:prod build:css update:md",
    "release": "run-s build:prod build:css update:md build:doc",
    "demo": "run-s build:lib build:css start",
    "build:doc": "run-s build compodoc",
    "compodoc": "compodoc -p projects/angular2-draggable/tsconfig.lib.json -d dist/docs/api-doc --theme stripe",
    "serve:doc": "compodoc -p projects/angular2-draggable/tsconfig.lib.json -s"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "font-awesome": "^4.7.0",
    "ngx-bootstrap": "3.0.1",
    "ngx-markdown": "^7.0.3",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@compodoc/compodoc": "^1.1.9",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bootstrap": "^4.1.1",
    "codelyzer": "~4.5.0",
    "cpr": "^3.0.1",
    "html-loader": "^0.5.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^4.2.0",
    "npm-run-all": "^4.1.3",
    "protractor": "~5.3.0",
    "raw-loader": "^0.5.1",
    "ts-node": "~7.0.0",
    "tsickle": ">=0.29.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6"
  }
}
